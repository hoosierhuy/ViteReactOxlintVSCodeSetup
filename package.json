{
  "name": "vitereactoxlintvscodesetup",
  "private": true,
  "version": "0.0.0",
  "description": "Helping developers with a Vite/React/Oxlint setup template",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "confetti-js": "0.0.18",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "oxlint": "^1.15.0",
    "oxlint-tsgolint": "^0.2.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite": "^7.1.2"
  }
}
